}
.infoSelect span{
	cursor: pointer;
	display:inline-block;
	padding-right:1vw;
	position:relative;
}
.infoSelect span:after{
	position:absolute;
	content:'';
	display:inline-block;
	right:0vw;
	top:2.4vh;
	border: 0.4vw solid transparent;
	border-top-color: #fff;
	transition: all .2s;
	-webkit-transition: all .2s
}
.line_h_white{
	width:1px;
	height:1.85vh;
	display:inline-block;
	background:#fff;
}
.Down{
	position:absolute;
	left:50%;
	top:5.4vh;
	width:100%;
	display:none;
	width:6.2vw;
	margin-left:-3.1vw;
}
.Down li{
	color:#fff;
	line-height:3.4vh;
	font-size:0.72vw;
	border-bottom:1px solid #2b3e5f;
	background:rgba(29,30,36,.8);
}
.Down li a{
	color:#fff;
}
.Down li:hover{
	background:rgba(43,62,95,.8);
}
.Down li:last-child{
	border:none;
}
.infoSelect:hover .Down{
	display:block;
}
.infoSelect:hover span:after{
	border: 0.4vw solid transparent;
	border-bottom-color: #fff;
	top:1.6vh;
}
.topTxt{
	display: flex;
	align-items:center;
	/*justify-content:center;*/
