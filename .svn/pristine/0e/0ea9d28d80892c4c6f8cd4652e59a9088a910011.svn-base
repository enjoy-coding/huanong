.topContent{
	text-align: center;
	position:fixed;
	z-index: 1002;
	top:0;
	left:0;
	right:0;
	display:block;
	width:100%;
	height:8vh;
	/*background:rgba(2,18,5,.8) url(../images/indexTopBg.png) no-repeat center;*/
