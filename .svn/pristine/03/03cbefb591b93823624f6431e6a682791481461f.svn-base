html,body,ul,ol,li,p,h1,h2,h3,h4,h5,h6,form,fieldset,table,td,img,div,dl,dt,dd,
input {
    margin: 0px;
    padding: 0px;
    font-weight: normal;
}
html,body{
    overflow: hidden;
    width: 100%;
    height: 100%;
}
body {
    font-family: "Microsoft YaHei", "Helvetica Neue", Helvetica,
    "PingFang SC", "Hiragino Sans GB", Arial, SimSun, sans-serif;
}
i{
    font-style: normal;
}
li {
    list-style: none;
}
.cp{
    cursor: pointer;
}
.hide{
    display:none;
}
.vh{
    height:100%
}
.oh{
    overflow: hidden;
}
.show{
    display:block;
}
.fl {
    float: left;
}

.fr {
    float: right;
}

.pr {
    position: relative;
}
.pa {
    position: absolute;
}
.pf{
    position:fixed;
}
.clr::after {
    clear: both;
    content: " ";
    display: block;
    height: 0;
    line-height: 0;
    visibility: hidden;
}
.txtC{
    text-align: center;
}
.txtR{
    text-align: right;
}


/* 设置滚动条的样式 */
::-webkit-scrollbar {
    width:3px;
    height: 3px;
}
/* 滚动槽 */
::-webkit-scrollbar-track {
    border-radius: 10px;
    background-color:rgba(46,111,187,1);
}
/* 滚动条滑块 */
::-webkit-scrollbar-thumb {
    width: 3px;
    height: 3px;
    background-color: #19c2c4;
    border-radius: 4px
}
