<!DOCTYPE html>
<html lang="zh" xmlns:th="http://www.thymeleaf.org" >
<head>
    <th:block th:include="include :: header('修改居民')" />
</head>
<body class="white-bg">
<div class="wrapper wrapper-content animated fadeInRight ibox-content">
    <form class="form-horizontal m" id="form-JM-edit" th:object="${JM}">
        <input id="jmid" name="jmid" th:field="*{jmid}" type="hidden">
        <div class="form-group">
            <label class="col-sm-3 control-label">居民id：</label>
            <div class="col-sm-8">
                <input name="jmid" th:field="*{jmid}" class="form-control" type="text">
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">姓名：</label>
            <div class="col-sm-8">
                <input name="xm" th:field="*{xm}" class="form-control" type="text">
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">性别：</label>
            <div class="col-sm-8">
                <input name="xb" th:field="*{xb}" class="form-control" type="text">
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">电话：</label>
            <div class="col-sm-8">
                <input name="dh" th:field="*{dh}" class="form-control" type="text">
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">年龄：</label>
            <div class="col-sm-8">
                <input name="nl" th:field="*{nl}" class="form-control" type="text">
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">籍贯：</label>
            <div class="col-sm-8">
                <input name="jg" th:field="*{jg}" class="form-control" type="text">
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">户籍性质：</label>
            <div class="col-sm-8">
                <input name="hjxz" th:field="*{hjxz}" class="form-control" type="text">
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">民族：</label>
            <div class="col-sm-8">
                <input name="mz" th:field="*{mz}" class="form-control" type="text">
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">政治面貌：</label>
            <div class="col-sm-8">
                <input name="zzmm" th:field="*{zzmm}" class="form-control" type="text">
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">学历：</label>
            <div class="col-sm-8">
                <input name="xl" th:field="*{xl}" class="form-control" type="text">
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">是否租户(是,否)：</label>
            <div class="col-sm-8">
                <select name="sfzh" class="form-control m-b" th:with="type=${@dict.getType('zhsq_sfzh')}">
                    <option th:each="dict : ${type}" th:text="${dict.dictLabel}" th:value="${dict.dictValue}" th:field="*{sfzh}"></option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">是否房主(是,否)：</label>
            <div class="col-sm-8">
                <select name="sffz" class="form-control m-b" th:with="type=${@dict.getType('zhsq_sffz')}">
                    <option th:each="dict : ${type}" th:text="${dict.dictLabel}" th:value="${dict.dictValue}" th:field="*{sffz}"></option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">房屋id：</label>
            <div class="col-sm-8">
                <input name="fwid" th:field="*{fwid}" class="form-control" type="text">
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">楼栋id：</label>
            <div class="col-sm-8">
                <input name="ldid" th:field="*{ldid}" class="form-control" type="text">
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">住址：</label>
            <div class="col-sm-8">
                <input name="zz" th:field="*{zz}" class="form-control" type="text">
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">是否独居老人(是,否)：</label>
            <div class="col-sm-8">
                <select name="sfdj" class="form-control m-b" th:with="type=${@dict.getType('zhsq_sfdj')}">
                    <option th:each="dict : ${type}" th:text="${dict.dictLabel}" th:value="${dict.dictValue}" th:field="*{sfdj}"></option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">是否空巢老人(是,否)：</label>
            <div class="col-sm-8">
                <select name="sfkc" class="form-control m-b" th:with="type=${@dict.getType('zhsq_sfkc')}">
                    <option th:each="dict : ${type}" th:text="${dict.dictLabel}" th:value="${dict.dictValue}" th:field="*{sfkc}"></option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">是否刑满释放(是,否)：</label>
            <div class="col-sm-8">
                <select name="sfxmsf" class="form-control m-b" th:with="type=${@dict.getType('zhsq_sfxmsf')}">
                    <option th:each="dict : ${type}" th:text="${dict.dictLabel}" th:value="${dict.dictValue}" th:field="*{sfxmsf}"></option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">是否退役军人(是,否)：</label>
            <div class="col-sm-8">
                <select name="sftyjr" class="form-control m-b" th:with="type=${@dict.getType('zhsq_sftyjr')}">
                    <option th:each="dict : ${type}" th:text="${dict.dictLabel}" th:value="${dict.dictValue}" th:field="*{sftyjr}"></option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">是否低保(是,否)：</label>
            <div class="col-sm-8">
                <select name="sfdb" class="form-control m-b" th:with="type=${@dict.getType('zhsq_sfdb')}">
                    <option th:each="dict : ${type}" th:text="${dict.dictLabel}" th:value="${dict.dictValue}" th:field="*{sfdb}"></option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">是否残疾人(是,否)：</label>
            <div class="col-sm-8">
                <select name="sfcj" class="form-control m-b" th:with="type=${@dict.getType('zhsq_sfcjr')}">
                    <option th:each="dict : ${type}" th:text="${dict.dictLabel}" th:value="${dict.dictValue}" th:field="*{sfcj}"></option>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-3 control-label">备注：</label>
            <div class="col-sm-8">
                <input name="remark" th:field="*{remark}" class="form-control" type="text">
            </div>
        </div>
    </form>
</div>
<th:block th:include="include :: footer" />
<script type="text/javascript">
    var prefix = ctx + "smartcommunity/JM";
    $("#form-JM-edit").validate({
        focusCleanup: true
    });

    function isAdd() {
        return $("#jmid").val() ? false : true;
    }

    function submitHandler() {
        if ($.validate.form()) {
            $.operate.save({
                url: prefix + (isAdd() ? "/add" : "/edit"),
                data: $('#form-JM-edit').serialize(),
                success: function(result, _option) {
                    if (isAdd()) {
                        if (result.data.jmid) {
                            top.formmsg = result.msg;
                            location.href = prefix + "/edit/" + result.data.jmid;
                        } else {
                            $.modal.msgError("没有返回数据：jmid");
                        }
                    } else {
                        $.modal.msgSuccess(result.msg);
                    }
                }
            });
        }
    }
</script>
</body>
</html>