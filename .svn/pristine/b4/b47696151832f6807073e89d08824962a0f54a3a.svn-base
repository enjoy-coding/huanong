package com.feather.aupipes.controller;

import com.feather.common.core.controller.BaseController;
import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;

/**
 * 移动端 - 首页
 */
@Controller
@RequestMapping("/mobile")
public class AupMobileController extends BaseController {
    private String prefix = "/aupipes/mobile";

    /**
     * 入口
     */
    @GetMapping("/index")
    public String index() {
        return prefix + "/index";
    }

    /**
     * 主页
     */
    @GetMapping("/home")
    public String home() {
        return prefix + "/home";
    }

    /**
     * 搜索页面
     */
    @GetMapping("/search")
    public String search() {
        return prefix + "/html/search";
    }

    /**
     * 搜索结果详情
     */
    @GetMapping("/searchDetail")
    public String searchDetail() {
        return prefix + "/html/searchDetail";
    }

    /**
     * 阀门关系详情
     */
    @GetMapping("/tapDetail")
    public String tapDetail() {
        return prefix + "/html/tapDetail";
    }

    /**
     * 维修档案
     */
    @GetMapping("/wxda")
    public String wxda() {
        return prefix + "/html/wxda";
    }

    /**
     * 维修档案详情
     */
    @GetMapping("/wxdaDetail")
    public String wxdaDetail() {
        return prefix + "/html/wxdaDetail";
    }

    /**
     * 巡检列表
     */
    @GetMapping("/xjlb")
    public String xjlb() {
        return prefix + "/html/xjlb";
    }

    /**
     * 巡检信息详情
     */
    @GetMapping("/xjlbDetail")
    public String xjlbDetail() {
        return prefix + "/html/xjlbDetail";
    }

    /**
     * 预警信息
     */
    @GetMapping("/yjxx")
    public String yjxx() {
        return prefix + "/html/yjxx";
    }

    /**
     * 预警信息详情
     */
    @GetMapping("/yjxxDetail")
    public String yjxxDetail() {
        return prefix + "/html/yjxxDetail";
    }

    /**
     * 运行监控
     */
    @GetMapping("/yxjk")
    public String yxjk() {
        return prefix + "/html/yxjk";
    }

    /**
     * 运行监控详情
     */
    @GetMapping("/yxjkDetail")
    public String yxjkDetail() {
        return prefix + "/html/yxjkDetail";
    }

    /**
     * 开始巡检
     */
    @GetMapping("/ksxj")
    public String ksxj() {
        return prefix + "/html/ksxj";
    }

    /**
     * 开挖分析
     */
    @GetMapping("/fx")
    public String fx() {
        return prefix + "/html/fx";
    }

    /**
     * 能耗监管
     */
    @GetMapping("/nhjg")
    public String nhjg() {
        return prefix + "/html/nhjg";
    }

    /**
     * 搜索结果
     */
    @GetMapping("/searchResult")
    public String searchResult() {
        return prefix + "/html/searchResult";
    }

    /**
     * 停水分析结果
     */
    @GetMapping("/gftsDetail")
    public String gftsDetail() {
        return prefix + "/html/gftsDetail";
    }

    /**
     * 拉闸限电结果
     */
    @GetMapping("/lzxdDetail")
    public String lzxdDetail() {
        return prefix + "/html/lzxdDetail";
    }

    /**
     * 水电用户楼栋信息
     */
    @GetMapping("/sdyhDetail")
    public String sdyhDetail() {
        return prefix + "/html/sdyhDetail";
    }

    /**
     * 水电用户楼栋信息
     */
    @GetMapping("/leakDetail")
    public String leakDetail() {
        return prefix + "/html/leakDetail";
    }

}
