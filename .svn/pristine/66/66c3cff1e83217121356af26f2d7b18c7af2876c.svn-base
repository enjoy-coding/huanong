<!DOCTYPE html>
<html lang="zh" xmlns:th="http://www.thymeleaf.org">

<head>
    <th:block th:include="include :: header('修改志愿者任务表')" />
    <link th:href="@{/bus/lpscommunity/css/uploadImg.css}" rel="stylesheet"/>
</head>

<body class="white-bg">
    <div class="wrapper wrapper-content animated fadeInRight ibox-content">
        <form class="form-horizontal m" id="form-TaskVolunteer-edit" th:object="${scTaskVolunteer}">
            <div class="row">
                <div class="col-sm-12">
                    <div class="form-group">
                        <label class="col-sm-2 control-label">标题：</label>
                        <div class="col-sm-8">
                            <input id="partyBuildTitle" name="partyBuildTitle"
                                th:field="${scTaskVolunteer.parkrent.parkrentTitle}" class="form-control" type="text"
                                disabled>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-4 control-label">类型：</label>
                        <div class="col-sm-6">
                            <select name="partyBuildType" id="partyBuildType" class="form-control m-b"
                                th:with="type=${@dict.getType('sc_task_state1')}" disabled>
                                <option th:each="dict : ${type}" th:text="${dict.dictLabel}"
                                    th:value="${dict.dictValue}" th:field="${scTaskVolunteer.parkrent.parkrentTitle}">
                                </option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-2 control-label">负责人：</label>
                        <div class="col-sm-6">
                            <input id="partyBuildSource" name="partyBuildSource"
                                th:field="${scTaskVolunteer.parkrent.parkrentName}" class="form-control" type="text"
                                disabled>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="form-group">
                        <label class="col-sm-2 control-label">图片：</label>
                        <div class="col-sm-8">
                            <div id="Pic_pass">
                                <div class="picDiv">
                                    <div class='imageDiv' th:each="f:${scFileInfoList}">
                                        <img th:src="${f.filePath}" onclick="imageView(this)" style="cursor:pointer;" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <h4 class="form-header h4">志愿者信息</h4>
            <div class="row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-4 control-label">头像：</label>
                        <div class="col-sm-6">
                            <img th:src="@{${sysFilePath}}" class="img_head" alt="User Image">
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-2 control-label"></label>
                        <div class="col-sm-8">

                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-4 control-label">姓名：</label>
                        <div class="col-sm-6">
                            <input id="partyBuildSource" name="partyBuildSource"
                                th:field="${scTaskVolunteer.parkrent.parkrentName}" class="form-control" type="text"
                                disabled>
                        </div>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-4 control-label">性别：</label>
                        <div class="col-sm-6">
                            <select class="form-control m-b" th:with="type=${@dict.getType('sys_user_sex')}" disabled>
                                <option th:each="dict : ${type}" th:text="${dict.dictLabel}"
                                    th:value="${dict.dictValue}" th:field="${scTaskVolunteer.volunteer.volunteerSex}">
                                </option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-4 control-label">电话：</label>
                        <div class="col-sm-6">
                            <input th:field="${scTaskVolunteer.volunteer.volunteerTel}" class="form-control" type="text"
                                disabled>
                        </div>

                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group">
                        <label class="col-sm-4 control-label">出生日期：</label>
                        <div class="col-sm-6">
                            <input th:field="${scTaskVolunteer.volunteer.volunteerBir}" class="form-control" type="text"
                                disabled>
                        </div>

                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-sm-12">
                    <div class="form-group">
                        <label class="col-sm-2 control-label">地址：</label>
                        <div class="col-sm-8">
                            <input id="partyBuildTitle" name="partyBuildTitle"
                                th:field="${scTaskVolunteer.volunteer.volunteerAddress}" class="form-control"
                                type="text" disabled>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="form-group">
                        <label class="col-sm-2 control-label">职业：</label>
                        <div class="col-sm-8">
                            <input id="partyBuildTitle" name="partyBuildTitle"
                                th:field="${scTaskVolunteer.volunteer.volunteerProfession}" class="form-control"
                                type="text" disabled>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="form-group">
                        <label class="col-sm-2 control-label">个人简介：</label>
                        <div class="col-sm-8">
                            <textarea disabled
                                class="form-control">[[${scTaskVolunteer.volunteer.volunteerSynopsis}]]</textarea>
                        </div>
                    </div>
                </div>
            </div>
            <h4 class="form-header h4">审核信息</h4>
            <div class="row">
                <div class="col-sm-12">
                    <div class="form-group">
                        <label class="col-sm-2 control-label">是否通过：</label>
                        <div class="col-sm-8">
                            <select name="auditPassState" disabled class="form-control m-b"
                                th:with="type=${@dict.getType('sc_audit_type')}">
                                <option th:each="dict : ${type}" th:text="${dict.dictLabel}"
                                    th:value="${dict.dictValue}" th:field="${scTaskVolunteer.auditPassState}">
                                </option>
                            </select>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-12">
                    <div class="form-group">
                        <label class="col-sm-2 control-label">内容：</label>
                        <div class="col-sm-8">
                            <textarea name="auditContent" maxlength="500" class="form-control" rows="3"
                                disabled>[[${scTaskVolunteer.auditContent}]]</textarea>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>

<th:block th:include="include :: footer" />
<script type="text/javascript">
        var prefix = ctx + "sc/TaskVolunteer";
        $("#form-TaskVolunteer-edit").validate({
            focusCleanup: true
        });

        function isAdd() {
            return $("#tvId").val() ? false : true;
        }

        function submitHandler() {
            if ($.validate.form()) {
                $.operate.save({
                    url: prefix + (isAdd() ? "/add" : "/edit"),
                    data: $('#form-TaskVolunteer-edit').serialize(),
                    success: function (result, _option) {
                        if (isAdd()) {
                            if (result.data.tvId) {
                                top.formmsg = result.msg;
                                location.href = prefix + "/edit/" + result.data.tvId;
                            } else {
                                $.modal.msgError("没有返回数据：tvId");
                            }
                        } else {
                            $.modal.msgSuccess(result.msg);
                        }
                    }
                });
            }
        }
    </script>
</body>

</html>