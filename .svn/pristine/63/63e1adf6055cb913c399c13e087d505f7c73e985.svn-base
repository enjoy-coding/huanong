    border: 1px solid #2bdad1;
    background: rgba(1, 5, 23, .8);
    -webkit-box-shadow: inset 0 0 1px rgba(80,255,245,.8);
    -moz-box-shadow: inset 0 0 1px rgba(80,255,245,.8);
    box-shadow: inset 0 0 1px rgba(80,255,245,.8);
