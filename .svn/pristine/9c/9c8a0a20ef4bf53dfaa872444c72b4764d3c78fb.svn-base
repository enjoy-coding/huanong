     *
     * @param obj
     * @return
     */
    @Override
    public boolean equals(Object obj) {
        if (this == obj) {
            return true;
        }
        if (obj == null) {
            return false;
        }
        if (getClass() != obj.getClass()) {
            return false;
        }
        //Parts是我的实体类名称
        NTAgriculturalUniversityCDForEnergyByBuilding parts = (NTAgriculturalUniversityCDForEnergyByBuilding) obj;
        //code改成你需要的属性名
        if (month == null) {
            if (parts.month != null) {
                return false;
            }
        } else if (!month.equals(parts.month)) {
            return false;
        }
        return true;
    }


