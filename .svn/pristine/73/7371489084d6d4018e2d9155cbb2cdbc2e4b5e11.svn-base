<span class="open hide">展开</span>
<div class="bottomLayer searchResult" id="xjlbBottomLayer">
    <div class="shrink">
        <span class="close zs-closeOpen" id="shrinkClose" onclick="closeResultMore()">
            <i class="iconfont iconshousuoxiajiantou"></i>
        </span>
        <span class="zs-close" onclick="closeInfo()">
            <i class="iconfont iconguanbi pa clearBtn"></i>
        </span>
    </div>
    <form class="layui-form bottomLayerFrom" id="xjlbForm" onsubmit="return false;">
        <div class="layui-row layui-col-space5">
            <div class="layui-col-xs6 layui-col-sm4 layui-col-md3">
                <div class="layui-form-item">
                    <label class="layui-form-label">巡检人员:</label>
                    <div class="layui-input-block">
                        <select name="person" lay-filter="person">
                            <option value=""></option>
                            <option value="0">全选</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="layui-col-xs6 layui-col-sm4 layui-col-md3">
                <div class="layui-form-item">
                    <label class="layui-form-label">巡检状态:</label>
                    <div class="layui-input-block">
                        <select name="status" lay-filter="status">
                            <option value="">全部</option>
                            <option value="0">未巡</option>
                            <option value="1">在巡</option>
                            <option value="2">已巡</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="layui-col-xs6 layui-col-sm4 layui-col-md3">
                <div class="layui-form-item">
                    <label class="layui-form-label">任务时间:</label>
                    <div class="layui-input-block">
                        <input type="text" id="test3" class="layui-input" placeholder="开始时间" autocomplete="off">
                    </div>
                </div>
            </div>
            <div class="layui-col-xs4 layui-col-sm4 layui-col-md3">
                <div class="layui-form-item time">
                    <label class="layui-form-label">至:</label>
                    <div class="layui-input-block">
                        <input type="text" id="test4" class="layui-input" placeholder="结束时间" autocomplete="off">
                    </div>
                </div>
            </div>
            <div class="layui-col-xs2 layui-col-sm4 layui-col-md3">
                <div class="layui-form-item txtR">
                    <button class="layui-btn bg_green" lay-submit lay-filter="xjFormSearch">搜索</button>
                </div>
            </div>
        </div>
    </form>
    <div class="titleStyle1">查询结果</div>
    <div class="xjlbList" id="xjlbListBox">
        <ul id="xjListBox">

        </ul>
    </div>
    <div class="listMore">
        <i class="iconfont icongengduo"></i>
        <h5>查找更多</h5>
    </div>
</div>

